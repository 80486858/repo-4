{
  "name": "express-oauth2-jwt-bearer",
  "description": "Authentication middleware for Express.js that validates JWT bearer access tokens.",
  "version": "0.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest test --coverage",
    "lint": "eslint --fix --ext .ts ./src",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "build": "rollup -c"
  },
  "author": "Auth0 <support@auth0.com>",
  "homepage": "https://github.com/auth0/node-oauth2-jwt-bearer/tree/main/packages/express-oauth2-jwt-bearer",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@tsconfig/node12": "^1.0.7",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "eslint": "^7.24.0",
    "express": "^4.17.1",
    "got": "^11.8.2",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "nock": "^13.0.11",
    "prettier": "^2.2.1",
    "rollup": "^2.45.1",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^26.5.4",
    "tslib": "^2.2.0",
    "typescript": "^4.2.3"
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-ts",
    "resolver": "../access-token-jwt/resolver.js",
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/*"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "reporters": [
      "default",
      [ "jest-junit", { "suiteName": "express-oauth2-jwt-bearer", "outputDirectory": "../../test-results/express-oauth2-jwt-bearer" } ]
    ]
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
